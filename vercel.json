{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/gemini.js", "use": "@vercel/node" },
	{ "src": "api/fetchReviews.js", "use": "@vercel/node" },
	{ "src": "api/generatePDF.js", "use": "@vercel/node" }
	
  ],
  "routes": [
    { "src": "/api/gemini", "dest": "/api/gemini.js" },
	{ "src": "/api/fetchReviews.js", "dest": "/api/fetchReviews.js" },
	{ "src": "/api/generatePDF.js", "use": "/api/generatePDF.js" },
	
    { "src": "/(.*)", "dest": "/$1" }
  ]
}